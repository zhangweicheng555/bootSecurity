<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.boot.kaizen.dao.lte.LteLoadTestDao">

	
	
		<select id="findListByMenodeBID" resultType="LteLoadTest">
		 select *
		 from
		 lte_load_test  
		 where eNodeBID=#{mENodeBID}
		</select>
	
	<insert id="save"  useGeneratedKeys="true" keyProperty="id">
		insert into  lte_load_test
		(
			userId
			,eNodeBID
			,communityName
			,testDate
			,rsrpFtpUpImage
			,sinrFtpUpImage
			,upFtpRateImage
			,rsrpFtpDownImage
			,sinrFtpDownImage
			,downFtpRateImage
			,sinrThresholdImage
			,rsrpThresholdImage
			,ftpRateThresholdImage
			,roadLogFile
			,projId
			,createTime
		) 
		values
		(
			#{userId}
			,#{eNodeBID}
			,#{communityName}
			,#{testDate}
			,#{rsrpFtpUpImage}
			,#{sinrFtpUpImage}
			,#{upFtpRateImage}
			,#{rsrpFtpDownImage}
			,#{sinrFtpDownImage}
			,#{downFtpRateImage}
			,#{sinrThresholdImage}
			,#{rsrpThresholdImage}
			,#{ftpRateThresholdImage}
			,#{roadLogFile}
			,#{projId}
			,#{createTime}
		)
	</insert>
	
	

	
</mapper>