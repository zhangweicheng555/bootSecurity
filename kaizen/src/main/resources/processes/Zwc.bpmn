  <process id="myTest" name="测试邮件" isExecutable="true">
    <startEvent id="startevent1" name="Start"></startEvent>
    <userTask id="usertask1" name="任务1" activiti:assignee="zll"></userTask>
    <userTask id="usertask2" name="任务2"></userTask>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow3" sourceRef="usertask2" targetRef="endevent1"></sequenceFlow>
    <serviceTask id="mailtask1" name="Mail Task" activiti:type="mail">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${receive}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[尽快去请求处理任务2]]></activiti:string>
        </activiti:field>
        <activiti:field name="html">
          <activiti:expression><![CDATA[<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>吼吼</title>
</head>
<body>
    
    来了邮件了 ${receive}
    
</body>
</html>]]></activiti:expression>
        </activiti:field>
        <activiti:field name="charset">
          <activiti:string><![CDATA[UTF-8]]></activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow4" sourceRef="usertask1" targetRef="mailtask1"></sequenceFlow>
    <sequenceFlow id="flow5" sourceRef="mailtask1" targetRef="usertask2"></sequenceFlow>
    <receiveTask id="receivetask1" name="等待任务"></receiveTask>
    <sequenceFlow id="flow6" sourceRef="startevent1" targetRef="receivetask1"></sequenceFlow>
    <sequenceFlow id="flow7" sourceRef="receivetask1" targetRef="usertask1"></sequenceFlow>
  </process>
